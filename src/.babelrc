{
  "babelrc": false,
  "presets": [
    "es2015-rollup"
  ],
  "plugins": [
    "syntax-jsx",
    [
      "transform-react-jsx",
      {
        "pragma": "html"
      }
    ],
    "transform-object-rest-spread"
  ],
  "env": {
    "coverage": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
