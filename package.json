{
  "name": "snabbdom-material-components",
  "version": "0.2.0",
  "author": "Daniel Lopes <lopsch@googlemail.com>",
  "description": "Material Components for the web for Snabbdom Virtual DOM",
  "main": "lib/index.js",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "keywords": [
    "snabbdom",
    "virtual-dom",
    "jsx",
    "mdc",
    "material"
  ],
  "bugs": "https://github.com/lopsch/snabbdom-material-components/issues",
  "repository": "https://github.com/lopsch/snabbdom-material-components",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "benchmark": "2.1.4",
    "chai": "4.1.0",
    "chai-arrays": "1.0.0",
    "chai-asserttype": "1.0.5",
    "chalk": "2.0.1",
    "cross-spawn": "5.1.0",
    "eslint": "4.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.0",
    "html-looks-like": "1.0.3",
    "jsdoc": "3.5.3",
    "jsdom": "11.1.0",
    "mocha": "3.4.2",
    "mochawesome": "2.3.0",
    "nyc": "11.1.0",
    "prettier": "1.5.3",
    "snabbdom": "0.6.9",
    "snabbdom-chai": "1.0.2",
    "snabbdom-to-html": "3.2.0"
  },
  "dependencies": {
    "@material/button": "0.3.10",
    "@material/card": "0.2.5",
    "@material/checkbox": "0.4.1",
    "@material/dialog": "0.3.4",
    "@material/fab": "0.3.12",
    "@material/form-field": "0.2.10",
    "@material/linear-progress": "0.1.5",
    "@material/radio": "0.2.8",
    "@material/switch": "0.1.10",
    "snabbdom-jsx-pragma": "*",
    "typestyle": "1.3.7"
  },
  "scripts": {
    "test-dev-min": "export NODE_ENV=test && export REPORTER=min && node -r babel-register scripts/test/test.dev.js",
    "test-dev": "export NODE_ENV=test && node -r babel-register scripts/test/test.dev.js",
    "test-ci": "export NODE_ENV=test && export BABEL_ENV=ci && node -r babel-register scripts/test/test.ci.js",
    "coverage": "node -r babel-register scripts/coverage/coverage.js",
    "doc": "node -r babel-register scripts/doc/doc.js",
    "dist-dev": "export NODE_ENV=test && node -r babel-register scripts/build/build.dev.js",
    "dist-ci": "export NODE_ENV=production && node -r babel-register scripts/build/build.ci.js",
    "lint-dist": "yarn run lint && yarn run dist-ci",
    "ci": "yarn run lint && yarn run test-ci && yarn run coverage && yarn run dist-ci && yarn run doc && yarn run lib",
    "lib": "node -r babel-register scripts/build/lib.js",
    "lint": "node -r babel-register scripts/linting/linting.js",
    "prettier": "node -r babel-register scripts/prettier/prettier.js",
    "format": "node -r babel-register scripts/prettier/prettier.format.js && node -r babel-register scripts/linting/linting.fix.js",
    "perf": "node -r babel-register perf/index.perf.js"
  }
}
