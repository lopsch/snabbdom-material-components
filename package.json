{
  "name": "snabbdom-material-components",
  "version": "0.5.6",
  "author": "Daniel Lopes <lopsch@googlemail.com>",
  "description": "Material Components for the web for Snabbdom Virtual DOM",
  "main": "lib/index.js",
  "module": "lib/es6/index.js",
  "license": "MIT",
  "keywords": [
    "snabbdom",
    "virtual-dom",
    "jsx",
    "mdc",
    "material"
  ],
  "bugs": "https://github.com/lopsch/snabbdom-material-components/issues",
  "repository": "https://github.com/lopsch/snabbdom-material-components",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-minify": "0.2.0",
    "babel-register": "6.26.0",
    "benchmark": "2.1.4",
    "chai": "4.1.1",
    "chai-arrays": "1.1.0",
    "chai-asserttype": "1.0.5",
    "chalk": "2.1.0",
    "cross-spawn": "5.1.0",
    "eslint": "4.4.1",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.2.1",
    "eslint-plugin-standard": "3.0.1",
    "fs-extra": "4.0.1",
    "html-looks-like": "1.0.3",
    "jsdoc": "3.5.4",
    "jsdom": "11.1.0",
    "mocha": "3.5.0",
    "mochawesome": "2.3.0",
    "nyc": "11.1.0",
    "prettier": "1.5.3",
    "rollup": "0.47.6",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-babel-minify": "3.1.2",
    "rollup-plugin-commonjs": "8.1.0",
    "rollup-plugin-eslint": "4.0.0",
    "rollup-plugin-multi-entry": "2.0.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-watch": "4.3.1",
    "snabbdom": "0.7.0",
    "snabbdom-chai": "1.0.2",
    "snabbdom-to-html": "3.2.0"
  },
  "dependencies": {
    "@material/button": "0.3.11",
    "@material/card": "0.2.6",
    "@material/checkbox": "0.4.2",
    "@material/dialog": "0.3.5",
    "@material/fab": "0.3.13",
    "@material/form-field": "0.2.11",
    "@material/icon-toggle": "0.1.16",
    "@material/linear-progress": "0.1.6",
    "@material/radio": "0.2.9",
    "@material/slider": "0.2.2",
    "@material/switch": "0.1.11",
    "snabbdom-jsx-pragma": "0.2.5",
    "typestyle": "1.3.8"
  },
  "scripts": {
    "test:dev": "mocha --colors --require babel-register --watch test/**/*.js",
    "test:cover": "nyc mocha --colors --require babel-register --reporter mochawesome --reporter-options reportFilename=index,reportDir=mochawesome,quiet=false,enableCode=false test/**/*.js",
    "test": "mocha --colors --require babel-register test/**/*.js",
    "cover": "rm -rf istanbul mochawesome && npm run build:cover && npm run test:cover && npm run cover:check",
    "cover:check": "nyc check-coverage --lines 0 --functions 0 --branches 0 --statements 0",
    "doc": "rm -rf jsdoc && jsdoc -d jsdoc -r src",
    "build:perf": "rm -rf lib && export NODE_ENV=performance && rollup -c rollup.config.dev.js",
    "build:cover": "rm -rf lib && export NODE_ENV=development && export BABEL_ENV=coverage && rollup -c rollup.config.dev.js && unset BABEL_ENV",
    "build:dev": "rm -rf lib && export NODE_ENV=development && rollup -c rollup.config.dev.js -w",
    "build:ci": "npm run clean && npm run cover && npm run build && npm run doc",
    "build:modules": "babel src --out-dir lib",
    "build:prod": "rollup -c rollup.config.prod.js",
    "build": "rm -rf lib && export NODE_ENV=production && export BABEL_ENV=production && npm run build:modules && npm run build:prod",
    "fix": "prettier --write --print-width 80 --tab-width 2 --no-semi --single-quote --trailing-comma none --jsx-bracket-same-line --parser babylon src/**/*.js test/**/*.js && eslint --fix src/**/*.js test/**/*.js",
    "perf": "npm run build:perf && babel-node perf/index.perf.js",
    "clean": "rm -rf istanbul jsdoc lib mochawesome"
  }
}
